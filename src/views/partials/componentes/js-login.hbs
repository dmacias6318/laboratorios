    <script>

  $("#eye-password").click(function () {
    var text = $(".password").attr("type");
    if (text == "password") {
      $(".password").attr('type', "text");
    } else {
      $(".password").attr('type', "password");

    }

  })

  $("#eye-codigo").click(function () {
    var text = $("#codigo").attr("type");
    if (text == "password") {
      $("#codigo").attr('type', "text");
    } else {
      $("#codigo").attr('type', "password");

    }



  })


</script>